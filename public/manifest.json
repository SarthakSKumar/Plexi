{
  "manifest_version": 3,
  "name": "Plexi Productivity",
  "version": "0.0.1",
  "description": "An All-in-one productivity chrome extension for the modern age",
  "permissions": [
    "activeTab",
    "cookies",
    "declarativeContent",
    "scripting",
    "storage",
    "tabs"
  ],
  "host_permissions": ["https://chat.openai.com/*"],
  "action": {
    "default_popup": "index.html"
  }
}
